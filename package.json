{
  "version": "1.0.0",
  "name": "minecraft-bedrock-schemas",
  "description": "Minecraft Bedrock JSON Schemas for addons development.",
  "author": "llgava <llgavamt@gmail.com>",
  "types": "index.d.ts",
  "keywords": [
    "Minecraft",
    "Minecraft Development",
    "JSON Schemas",
    "TypeScript"
  ],
  "license": "MIT",
  "main": "./src/index.ts",
  "scripts": {
    "generate-schemas": "node -r tsconfig-paths/register scripts/GenerateSchema.js",
    "build": "webpack --progress && yarn post-build",
    "build:npm": "webpack --progress && npm post-build",
    "post-build": "eslint index.d.ts --fix",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only scripts/generate-schemas.ts",
    "lint": "yarn eslint . --ext .ts",
    "bump": "yarn upgrade --latest",
    "test": "jest"
  },
  "dependencies": {
    "chalk": "4.1.2",
    "ts-json-schema-generator": "^1.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.0.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-check-file": "^1.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.3.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.14.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.6.2",
    "typescript-declaration-webpack-plugin": "^0.2.2",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/llgava/minecraft-bedrock-schemas.git"
  }
}
