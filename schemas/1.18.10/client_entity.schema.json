{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/definitions/ClientEntitySchema",
  "definitions": {
    "ClientEntitySchema": {
      "type": "object",
      "properties": {
        "format_version": {
          "type": "string"
        },
        "minecraft:client_entity": {
          "$ref": "#/definitions/ClientEntity"
        }
      },
      "additionalProperties": false,
      "$id": "llgava.resource_packs.SoundsDefinition"
    },
    "ClientEntity": {
      "type": "object",
      "properties": {
        "description": {
          "$ref": "#/definitions/ClientEntityProperties"
        }
      },
      "required": ["description"],
      "additionalProperties": false
    },
    "ClientEntityProperties": {
      "type": "object",
      "properties": {
        "identifier": {
          "type": "string",
          "pattern": "^(?:(?:(?!minecraft|minecon))+[a-z]*:[a-z_]*)+$"
        },
        "render_controllers": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/RenderController"
          }
        },
        "enable_attachables": {
          "type": "boolean"
        },
        "hide_armor": {
          "type": "boolean"
        },
        "min_engine_version": {
          "type": "string"
        },
        "sound_effects": {
          "$ref": "#/definitions/SoundEffects"
        },
        "particle_emitters": {
          "$ref": "#/definitions/ParticleEmitters"
        },
        "queryable_geometry": {
          "type": "string"
        },
        "materials": {
          "$ref": "#/definitions/Materials"
        },
        "geometry": {
          "$ref": "#/definitions/Geometry"
        },
        "particle_effects": {
          "$ref": "#/definitions/ParticleEffects"
        },
        "textures": {
          "$ref": "#/definitions/ClientTextures"
        },
        "animations": {
          "$ref": "#/definitions/Animations"
        },
        "scripts": {
          "$ref": "#/definitions/ClientScripts"
        },
        "spawn_egg": {
          "$ref": "#/definitions/SpawnEgg"
        }
      },
      "additionalProperties": false
    },
    "RenderController": {
      "type": "string",
      "pattern": "^(?:controller.render.)+[a-z]+$"
    },
    "SoundEffects": {
      "type": "object",
      "additionalProperties": {
        "type": "string"
      }
    },
    "ParticleEmitters": {
      "type": "object",
      "additionalProperties": {
        "type": "string"
      }
    },
    "Materials": {
      "type": "object",
      "additionalProperties": {
        "type": "string"
      }
    },
    "Geometry": {
      "type": "object",
      "additionalProperties": {
        "type": "string",
        "pattern": "^(?:geometry.)+[a-z_]+$"
      }
    },
    "ParticleEffects": {
      "type": "object",
