{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/definitions/FlipbookTextures",
  "definitions": {
    "FlipbookTextures": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/MinecraftFlipbookTexture"
      },
      "$id": "llgava.resource_packs.FlipbookTextures"
    },
    "MinecraftFlipbookTexture": {
      "type": "object",
      "properties": {
        "atlas_index": {
          "type": "number"
        },
        "atlas_tile_variant": {
          "type": "number"
        },
        "blend_frames": {
          "type": "boolean"
        },
        "replicate": {
          "type": "number"
        },
        "ticks_per_frame": {
          "type": "number"
        },
        "flipbook_texture": {
          "type": "string"
        },
        "atlas_tile": {
          "type": "number"
        },
        "frames": {
          "type": "array",
          "items": {
            "type": "number"
          }
        }
      },
      "additionalProperties": false
    }
  }
}
